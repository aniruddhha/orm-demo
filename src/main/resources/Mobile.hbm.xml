<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ani.orm.xmlconfig.Mobile">
        <id name="mbId" type="long" column="mb_id">
            <generator class="native"/>
        </id>
        <property name="mbNum" type="string" column="mb_num" unique="true"/>
        <property name="version" type="int" column="mb_ver"/>
    </class>
</hibernate-mapping>